package example02;
import java.io.*;
//객체 입출력
/*
 * 
 */
class Point{
	int x;
	int y;
	
	Point(){}
	public Point(int x,int y) {
		this.x=x;
		this.y=y;
	}
	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "x="+x+" y="+y;
	}
}
public class Exam_03 {
	public static void main(String[] args) {
		Point p1 =new Point();
		
	}

}
